{% comment %} app plugins {% endcomment %}
    def __init__(self, plugin_name, ui, logger):
        self.__ui = ui
        self.__logger = logger
        self.__plugin_name = plugin_name

        self.__logger.debug("Initialized plugin")

    def run(self, name, foo="bar"):
