class {{ app.name.class }}Plugin:
{% comment %} app plugins {% endcomment %}
    def __init__(self, plugin_name, print, logger):
        self.__print = print
        self.__logger = logger
        self.__plugin_name = plugin_name

        self.__logger.debug("Initialized plugin")

    def run(self, name, foo="foo"):
        self.__print(f"Hello {name} from plugin {self.__plugin_name} with arg foo: {foo}")
